<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                    charset="UTF-8"
                    pattern="%d{HH:mm:ss.SSS} [%t] %-5level [%X{uuid}] %logger{36} - %message{nolookups}%n"
            />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>